#ident	"@(#)oamintf:devices/remove/Prototype	1.3"
!MENU=/usr/sadm/sysadm/menu
!INTFBIN=/usr/sadm/sysadm/bin

d intf $MENU/devices/remove 0755 bin bin
v intf $MENU/devices/remove/remove.menu 0644 bin bin
f intf $MENU/devices/remove/Form.disk 0644 bin bin
f intf $MENU/devices/remove/Text.rmdisk 0644 bin bin
f intf $MENU/devices/remove/Text.rmnok 0644 bin bin
f intf $MENU/devices/remove/Text.rmok 0644 bin bin
f intf $INTFBIN/rmdisk 0755 bin bin
f intf $MENU/devices/remove/Help 0644 bin bin

