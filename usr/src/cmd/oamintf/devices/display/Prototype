#ident	"@(#)oamintf:devices/display/Prototype	1.2"
!MENU=/usr/sadm/sysadm/menu
!INTFBIN=/usr/sadm/sysadm/bin

f intf $INTFBIN/dispdisk 0755 bin bin
d intf $MENU/devices/display 0755 bin bin
v intf $MENU/devices/display/display.menu 0644 bin bin
f intf $MENU/devices/display/Form.disk 0644 bin bin
f intf $MENU/devices/display/Text.display 0644 bin bin
f intf $MENU/devices/display/Text.disperr 0644 bin bin
f intf $MENU/devices/display/Help 0644 bin bin
