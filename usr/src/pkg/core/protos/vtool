#ident	"@(#)pkgcore:protos/vtool	1.4"

d vtool /usr/sadm/bin 755 bin bin
f vtool /usr/sadm/bin/puttext 555 bin bin

f vtool /usr/bin/ckint 555 bin bin
f vtool /usr/sadm/bin/valint 555 bin bin
f vtool /usr/sadm/bin/helpint 555 bin bin
f vtool /usr/sadm/bin/errint 555 bin bin

f vtool /usr/bin/ckitem 555 bin bin
f vtool /usr/sadm/bin/helpitem 555 bin bin
f vtool /usr/sadm/bin/erritem 555 bin bin

f vtool /usr/bin/ckpath 555 bin bin
f vtool /usr/sadm/bin/valpath 555 bin bin
f vtool /usr/sadm/bin/helppath 555 bin bin
f vtool /usr/sadm/bin/errpath 555 bin bin

f vtool /usr/bin/ckrange 555 bin bin
f vtool /usr/sadm/bin/valrange 555 bin bin
f vtool /usr/sadm/bin/helprange 555 bin bin
f vtool /usr/sadm/bin/errange 555 bin bin

f vtool /usr/bin/ckstr 555 bin bin
f vtool /usr/sadm/bin/valstr 555 bin bin
f vtool /usr/sadm/bin/helpstr 555 bin bin
f vtool /usr/sadm/bin/errstr 555 bin bin

f vtool /usr/bin/ckyorn 555 bin bin
f vtool /usr/sadm/bin/valyorn 555 bin bin
f vtool /usr/sadm/bin/helpyorn 555 bin bin
f vtool /usr/sadm/bin/erryorn 555 bin bin

f vtool /usr/bin/ckkeywd 555 bin bin

f vtool /usr/bin/cktime 555 bin bin
f vtool /usr/sadm/bin/valtime 555 bin bin
f vtool /usr/sadm/bin/helptime 555 bin bin
f vtool /usr/sadm/bin/errtime 555 bin bin

f vtool /usr/bin/ckdate 555 bin bin
f vtool /usr/sadm/bin/valdate 555 bin bin
f vtool /usr/sadm/bin/helpdate 555 bin bin
f vtool /usr/sadm/bin/errdate 555 bin bin

f vtool /usr/bin/ckuid 555 bin bin
f vtool /usr/bin/dispuid 555 bin bin
f vtool /usr/sadm/bin/valuid 555 bin bin
f vtool /usr/sadm/bin/helpuid 555 bin bin
f vtool /usr/sadm/bin/erruid 555 bin bin
f vtool /usr/sadm/bin/dispuid 555 bin bin

f vtool /usr/bin/ckgid 555 bin bin
f vtool /usr/bin/dispgid 555 bin bin
f vtool /usr/sadm/bin/valgid 555 bin bin
