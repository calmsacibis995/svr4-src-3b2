/* #ident	"@(#)boot:boot/mboot/mbld	11.2" */

/* mboot.b loader instruction file */

MEMORY	{
	mbcode:origin = 0x2004000, length = 512
        mbbss:origin = 0x2004200,length = 1024
	}
SECTIONS	{
	.text: { } > mbcode
	.data: { } > mbcode
	.data1: { } > mbcode
	.rodata: { } > mbcode
	.rodata1: { } > mbcode
	.bss:  { } > mbbss
	}
mboot.o
